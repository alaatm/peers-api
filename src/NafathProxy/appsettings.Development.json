{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "reverseProxy": {
    "routes": {
      "nafathProxy": {
        "clusterId": "azureCluster",
        "match": {
          "path": "/nafath-callback"
        }
      }
    },
    "clusters": {
      "azureCluster": {
        "destinations": {
          "peersDestination": {
            "address": "https://localhost:7285/"
          }
        }
      }
    }
  }
}
